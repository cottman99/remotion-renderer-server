{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "ACCESS_KEY_ID": "@access_key_id",
    "ACCESS_KEY_SECRET": "@access_key_secret",
    "OSS_REGION": "@oss_region",
    "OSS_BUCKET": "@oss_bucket"
  }
} 